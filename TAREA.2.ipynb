   "source": [
    "def newton2(fun,d_fun,x_i,err=1e-300):\n",
    "    while abs(fun(x_i)) >= err:\n",
    "        raiz=x_i-fun(x_i)/d_fun(x_i)\n",
    "        x_i = raiz\n",
    "    return raiz"
   ]
  }
